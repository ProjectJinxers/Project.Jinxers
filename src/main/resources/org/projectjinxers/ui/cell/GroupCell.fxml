<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1">
	<Label text="${controller.name}"/>
	<Label text="${controller.address}"/>
	<Label text="${controller.timestampTolerance}"/>
	<CheckBox fx:id="saveBox" text="Save" onAction="#saveToggled"/>
	<Label text="${controller.validModelState}">
	   <tooltip>
	       <Tooltip text="${controller.multihash}"/>
	   </tooltip>
	</Label>
</VBox>

